{
    "name": "drupal-composer/drupal-project",
    "description": "Project template for Drupal 8 projects with composer",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "",
            "role": ""
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "require": {
        "php": ">=5.6",
        "composer/installers": "^1.2",
        "cweagans/composer-patches": "^1.6.5",
        "drupal-composer/drupal-scaffold": "^2.5",
        "drupal/address": "^1.6",
        "drupal/backup_migrate": "^4.0",
        "drupal/conditional_fields": "^1.0@alpha",
        "drupal/console": "~1.0",
        "drupal/core": "^8.7.0",
        "drupal/devel": "^2.1",
        "drupal/devel_php": "^1.0",
        "drupal/disable_messages": "^1.1",
        "drupal/eck": "^1.0@alpha",
        "drupal/examples": "1.x-dev",
        "drupal/field_group": "^1.0",
        "drupal/front": "^1.0@beta",
        "drupal/inline_entity_form": "^1.0@RC",
        "drupal/menu_trail_by_path": "^1.1",
        "drupal/mobile_number": "^1.0@alpha",
        "drupal/paragraphs": "^1.8",
        "drupal/questions_answers": "^1.10",
        "drupal/range": "^1.1",
        "drupal/restui": "^1.17",
        "drupal/sendgrid_integration": "^1.2",
        "drupal/smtp": "^1.0@beta",
        "drupal/structure_sync": "^1.16",
        "drupal/superfish": "^1.3",
        "drupal/time_range": "^1.1",
        "drupal/views_autocomplete_filters": "^1.2",
        "drupal/workflow": "^1.1",
        "drupal/yearonly": "^1.3",
        "drush/drush": "^9.0.0",
        "vlucas/phpdotenv": "^2.4",
        "webflo/drupal-finder": "^1.0.0",
        "webmozart/path-util": "^2.3",
        "zaporylie/composer-drupal-optimizations": "^1.0"
    },
    "require-dev": {
        "webflo/drupal-core-require-dev": "^8.7.0"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ],
        "files": ["load.environment.php"]
    },
    "scripts": {
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ]
    },
    "extra": {
        "composer-exit-on-patch-failure": true,
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "installer-paths": {
            "web/core": ["type:drupal-core"],
            "web/libraries/{$name}": ["type:drupal-library"],
            "web/modules/contrib/{$name}": ["type:drupal-module"],
            "web/profiles/contrib/{$name}": ["type:drupal-profile"],
            "web/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/Commands/{$name}": ["type:drupal-drush"]
        },
        "drupal-scaffold": {
            "initial": {
                ".editorconfig": "../.editorconfig",
                ".gitattributes": "../.gitattributes"
            }
        }
    }
}
